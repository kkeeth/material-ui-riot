    "@riotjs/webpack-loader": "^4.0.3",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "chai": "^4.2.0",
    "css-loader": "^4.2.0",
