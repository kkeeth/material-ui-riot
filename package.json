{
  "name": "material-ui-riot",
  "version": "1.0.0",
  "description": "Set of material ui components for riot js.",
  "main": "build/js/riot-mui-min.js",
  "author": "Keeth Kuwahara",
  "scripts": {
    "build:dev": "webpack -c --mode development",
